{
  "name": "frontend-app-ora",
  "version": "0.1.0",
  "description": "Frontend application template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-app-ora.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "fedx-scripts formatjs extract",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk node_modules/.bin/fedx-scripts jest --runInBand --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-app-ora#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/frontend-app-ora/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-edx.org@2.1.3",
    "@edx/frontend-lib-special-exams": "^3.1.0",
    "@edx/frontend-platform": "8.3.5",
    "@edx/openedx-atlas": "^0.7.0",
    "@edx/react-unit-test-utils": "^3.0.0",
    "@edx/tinymce-language-selector": "1.1.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@openedx/paragon": "^21.13.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@tinymce/tinymce-react": "3.14.0",
    "@zip.js/zip.js": "^2.7.30",
    "axios": "^1.5.1",
    "classnames": "^2.3.2",
    "core-js": "3.41.0",
    "file-saver": "^2.0.5",
    "filesize": "^8.0.6",
    "jest-when": "^3.6.0",
    "moment": "^2.29.4",
    "pdfjs-dist": "^3.11.174",
    "prop-types": "15.8.1",
    "query-string": "^8.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-pdf": "^7.4.0",
    "react-redux": "7.2.9",
    "react-router": "6.28.2",
    "react-router-dom": "6.28.2",
    "redux": "4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "0.14.1",
    "tinymce": "5.10.9",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@openedx/frontend-build": "14.3.2",
    "@edx/reactifex": "^2.1.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.5.1",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4"
  },
  "overrides": {
    "@edx/frontend-lib-special-exams": {
      "@openedx/paragon": "21.13.1"
    },
    "@edx/frontend-platform": {
      "@openedx/paragon": "21.13.1"
    }
  }
}
